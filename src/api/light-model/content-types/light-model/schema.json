{
  "kind": "collectionType",
  "collectionName": "light_models",
  "info": {
    "singularName": "light-model",
    "pluralName": "light-models",
    "displayName": "LightModel"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "lightBrand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::light-brand.light-brand"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
