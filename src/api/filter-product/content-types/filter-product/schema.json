{
  "kind": "collectionType",
  "collectionName": "filter_products",
  "info": {
    "singularName": "filter-product",
    "pluralName": "filter-products",
    "displayName": "FilterProduct",
    "description": "Purflux filter products catalog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand": {
      "type": "string",
      "default": "PURFLUX",
      "required": true
    },
    "filterType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "oil",
        "air",
        "diesel",
        "cabin"
      ]
    },
    "reference": {
      "type": "string",
      "required": true
    },
    "fullReference": {
      "type": "string"
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "ean": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "internalSKU": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "uid",
      "targetField": "reference",
      "required": true
    },
    "img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "brandImg": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
