{
  "kind": "collectionType",
  "collectionName": "brands",
  "info": {
    "singularName": "brand",
    "pluralName": "brands",
    "displayName": "Brand"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "isActive": {
      "type": "boolean"
    },
    "models": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::model.model",
      "mappedBy": "brand"
    },
    "vehicle_type": {
      "type": "enumeration",
      "enum": ["car", "moto", "car-moto"],
      "default": "car"
    }
  }
}
